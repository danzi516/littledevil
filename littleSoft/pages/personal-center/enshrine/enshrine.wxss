page{
  background-color: #EBEBEB;
  width: 100%;
  height: 100%;
}

.enshrineWrap{
  width: 100%;
  height: 162rpx;
  background-color: white;
  display: flex;
  justify-content: space-between;
  border: 1px solid #E0E0E0;
}

.enshrineLeft{
  width: 90%;
  height: 162rpx;
  background-color: white;
  display: flex;
  justify-content: space-between;
}

.enshrineImages{
  width: 130rpx;
  height: 130rpx;
}

.enshrineImages-right{
  width: 12rpx;
  height: 22rpx;
}

.enshrine-classify{
  color: #808080;
  font-size: 26rpx;
  display: block;
  text-overflow:ellipsis; 
  white-space:nowrap; 
  overflow:hidden;
}

.enshrineImages-wrap{
  width: 34%;
  height: 162rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.enshrine-caip{
  width: 65%;
  height: 162rpx;
  letter-spacing: 2rpx;
  line-height: 49rpx;
}

.enshrine-name{
  font-size: 32rpx;
  color: #1E1E1E;
  display:block;
  text-overflow:ellipsis;
  white-space:nowrap;
  overflow:hidden;
}

.enshrine-dynamic{
  background-color: #FC5E2D;
  width: 62rpx;
  height: 30rpx;
  border-radius: 4rpx;
  color: white;
  font-size: 24rpx;
  padding: 0rpx 6rpx;
  margin-right: 20rpx;
}

.enshrine-vote{
  background-color: #FC2D59;
  width: 62rpx;
  height: 30rpx;
  border-radius: 4rpx;
  color: white;
  font-size: 24rpx;
  padding: 0rpx 6rpx;
}

.enshrineWrap-right{
  width: 5%;
  height: 162rpx;
  display: flex;
  align-items: center;
}