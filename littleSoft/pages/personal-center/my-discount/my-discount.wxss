@import "discount-template/discount-template.wxss";
page{
  background-color: #EBEBEB;
}
.placeholder {
  position: none;
  height: 90%;
}
.conceal-dot{
  width: 20rpx;
  height: 20rpx;
  border-radius: 50%;
  background-color: #EBEBEB;
  position: absolute;
  top: 175rpx;
  left: 22rpx;
}

.conceal-dot-right{
  width: 20rpx;
  height: 20rpx;
  border-radius: 50%;
  background-color: #EBEBEB;
  position: absolute;
  top: 175rpx;
  left: 709rpx;
}

.already-past-due{
  width: 92%;
  height: 76rpx;
  background-color: white;
  margin-top: 40rpx;
  border-top-left-radius: 10rpx;
  border-top-right-radius: 10rpx;
  border-bottom-left-radius: 10rpx;
  border-bottom-right-radius: 10rpx;
  color: #555555;
  font-size: 26rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  letter-spacing: 2px;
}

.already-past-wrap{
  width: 100%;
  display:flex;
  justify-content: center;
}

page {
  width: 100%;
  height: 100%;
}
